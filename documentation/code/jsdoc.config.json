{
  "opts": {
    "template": "node_modules/docdash"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [
      "documentation/code/template/static"
    ],
    "menu":{
      "Github repo": {
        "href":"https://github.com/NIAEFEUP/nijobs-be",
        "target":"_blank",
        "class":"menu-item",
        "id":"repository"
      }
    }
  },
  "templates": {
    "default": {
      "useLongnameInNav": true,
      "layoutFile": "documentation/code/template/tmpl/layout.tmpl",
      "staticFiles": {
        "include": [
          "documentation/code/template/static"
        ]
      }
    }
  }
}